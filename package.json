{
  "name": "remote-abap-compiler-cli",
  "version": "0.0.1",
  "description": "Remote ABAP compilation",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "tslint": "^5.17.0",
    "clean": "rimraf ./dist",
    "build": "tsc && npm run lint",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "lint": "tslint -c tslint.json 'lib/**/*.ts'"
  },
  "bin": {
    "abapc": "./bin/run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.wdf.sap.corp/d054287/remote-abap-compiler-cli.git"
  },
  "keywords": [
    "SAP",
    "R/3",
    "ABAP",
    "compiler"
  ],
  "author": "SAP",
  "license": "ISC",
  "bugs": {
    "url": "https://github.wdf.sap.corp/d054287/remote-abap-compiler-cli/issues"
  },
  "homepage": "https://github.wdf.sap.corp/d054287/remote-abap-compiler-cli#readme",
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.12",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@oclif/command": "^1.5.15",
    "@oclif/config": "^1.13.0",
    "@oclif/plugin-help": "^2.2.0",
    "@types/read": "0.0.28",
    "read": "^1.0.7",
    "remote-abap-compiler": "^2.0.0"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "abapc",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "files": [
    "/bin",
    "/lib",
    "/dist"
  ]
}
